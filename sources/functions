# vi: ft=zsh

# jump to a project from anywhere
go () { cd ~/projects/$1; }
_go () { _files -W ~/projects -/; }
compdef _go go

# jump to a project from anywhere
ccb () { cd ~/Development/churchcommunitybuilder/app/$1; }
_ccb () { _files -W ~/Development/churchcommunitybuilder/app -/; }
compdef _ccb ccb

# noitifications
n () {
  osascript -e "display notification \"$1\" with title \"$2\""
}
