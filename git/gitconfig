[core]
  editor = vim
	excludesfile = /Users/kdurbin/.gitignore
[user]
	name = Kevin Durbin
	email = kevindurb@gmail.com
[color]
  status = auto
  branch = auto
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "status"]
  added = green
  changed = red
  untracked = cyan
[push]
	default = current
[rerere]
  enabled = 1
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[commit]
  template = /Users/kdurbin/.stCommitMsg
